{
	"area-percentarea-category": {
		"graph": {
			"aoi": ["geojson", "{user_json}"],
	        "reference_data": ["esri:server", "{layer_url}", "{user_json}"],
	        "dissolve-aoi": ["dissolve", "aoi"],
	        "project-aoi": ["project_local", "dissolve-aoi"],
	        "project-data": ["project_local", "reference_data"],
	        "intersect-aoi-data": ["intersect", "project-aoi",
	                                  "project-data"],
	        "intersect-area": ["get_intersect_area", "project-aoi",
	                           "intersect-aoi-data", "{unit}",
	                       	   "{category}"],
	        "intersect-area-percent": ["get_intersect_area_percent",
	                                   "project-aoi",
	                                   "intersect-aoi-data",
	                                   "{category}"]
		},
		"outputs": ["intersect-area", "intersect-area-percent"]
	},
	"area-percentarea": {
		"graph": {
			"aoi": ["geojson", "{user_json}"],
	        "reference_data": ["esri:server", "{layer_url}", "{user_json}"],
	        "dissolve-aoi": ["dissolve", "aoi"],
	        "dissolve-data": ["dissolve", "reference_data"],
	        "project-aoi": ["project_local", "dissolve-aoi"],
	        "project-data": ["project_local", "dissolve-data"],
	        "intersect-aoi-data": ["intersect", "project-aoi",
	                                  "project-data"],
	        "intersect-area": ["get_intersect_area", "project-aoi",
	                           "intersect-aoi-data", "{unit}"],
	        "intersect-area-percent": ["get_intersect_area_percent",
	                                   "project-aoi",
	                                   "intersect-aoi-data"]
		},
		"outputs": ["intersect-area", "intersect-area-percent"]
	}
}